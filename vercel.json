{
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/index.html" },
      { "src": "/api/(.*)", "dest": "/server.js" }
    ]
  }
  